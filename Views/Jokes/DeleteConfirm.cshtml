@model GroanZone.Models.Joke
<h2>Delete Joke</h2>
<h3>Are you sure you want to delete this joke? This action cannot be undone.</h3>
<p><strong>@Model.Setup</strong></p>
<form asp-action="Delete" asp-route-id="@Model.JokeId" method="post">
    @Html.AntiForgeryToken()
    <input type="hidden" name="id" value="@Model.JokeId" />
    <button class="btn btn-danger" type="submit">Yes, delete</button>
    <a class="btn btn-secondary" asp-action="Details" asp-route-id="@Model.JokeId">Cancel</a>
</form>